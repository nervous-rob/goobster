{
  "name": "goobster",
  "version": "1.0.0",
  "description": "Discord bot with adventure capabilities",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy-commands": "node deploy-commands.js",
    "start": "npm run deploy-commands && node index.js",
    "dev": "nodemon index.js",
    "db-init": "node initDb.js",
    "lint": "eslint .",
    "test:integration": "jest --config jest.integration.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/storage-blob": "^12.17.0",
    "@discordjs/builders": "^1.7.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^2.2.0",
    "@discordjs/voice": "^0.16.1",
    "axios": "^1.7.9",
    "canvas": "^3.1.0",
    "concurrently": "^8.0.0",
    "cors": "^2.8.5",
    "cron-parser": "^5.0.4",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "libsodium-wrappers": "^0.7.15",
    "microsoft-cognitiveservices-speech-sdk": "^1.34.0",
    "mssql": "^10.0.2",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.0",
    "openai": "^4.77.3",
    "play-dl": "^1.9.7",
    "prism-media": "^1.3.5",
    "replicate": "^0.27.1",
    "sharp": "^0.32.6",
    "sodium-native": "^4.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "@google/genai": "^0.2.0",
    "mime": "^3.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mssql": "^9.1.4",
    "@types/node": "^22.10.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "mock-socket": "^9.3.1",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "utils/**/*.{js,ts}",
      "commands/**/*.{js,ts}",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
